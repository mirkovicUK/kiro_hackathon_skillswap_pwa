{
  "name": "skillswap",
  "version": "1.0.0",
  "description": "Neighborhood skill exchange PWA with mandatory coffee meetings",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:props": "vitest run --grep property"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "better-sqlite3": "^12.5.0",
    "concurrently": "^8.2.2",
    "fast-check": "^3.15.0",
    "postcss": "^8.4.35",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.0",
    "vitest": "^1.3.0"
  }
}
